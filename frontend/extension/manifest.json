{
  "manifest_version": 3,
  "name": "Accuracy Level",
  "version": "1.0",
  "description": "Extension.",
  "permissions": [
    "tabs", 
    "scripting", 
    "activeTab"
  ],
  "host_permissions": ["<all_urls>"],
    "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }
    ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
